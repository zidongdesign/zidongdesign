IAAA,CAAA;AACA,KAHA;AAIA,IAAA,QAAA,EAAA,oBAAA;AACA,UAAA,CAAA,GAAA,KAAA,WAAA;AACA,UAAA,WAAA,GAAA,KAAA,WAAA,CAAA,IAAA,GAAA,WAAA,CAAA,OAAA,GAAA,MAAA,CAAA,UAAA,IAAA,EAAA;AACA,eAAA,IAAA,CAAA,WAAA,IAAA,IAAA,CAAA,aAAA,IAAA,CAAA,IAAA,CAAA,IAAA,CAAA;AACA,OAFA,CAAA;AAGA,UAAA,SAAA,GAAA,WAAA,CAAA,MAAA;;AACA,WAAA,IAAA,EAAA,GAAA,CAAA,EAAA,EAAA,GAAA,SAAA,GAAA,CAAA,EAAA,EAAA,EAAA,EAAA;AACA,aAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,SAAA,GAAA,CAAA,GAAA,EAAA,EAAA,CAAA,EAAA,EAAA;AACA,cAAA,WAAA,CAAA,CAAA,CAAA,CAAA,MAAA,GAAA,WAAA,CAAA,CAAA,GAAA,CAAA,CAAA,CAAA,MAAA,EAAA;AACA,gBAAA,IAAA,GAAA,WAAA,CAAA,CAAA,CAAA;AACA,YAAA,WAAA,CAAA,CAAA,CAAA,GAAA,WAAA,CAAA,CAAA,GAAA,CAAA,CAAA;AACA,YAAA,WAAA,CAAA,CAAA,GAAA,CAAA,CAAA,GAAA,IAAA;AACA;AACA;AACA;;AACA,aAAA,WAAA;AACA;AApBA,GAvBA;AA6CA,EAAA,OA7CA,qBA6CA,CACA,CA9CA;AA+CA,EAAA,OAAA,EAAA;AACA,IAAA,iBADA,+BACA;AACA,WAAA,KAAA,CAAA,YAAA,CAAA,IAAA;AACA,KAHA;AAIA,IAAA,4BAJA,wCAIA,QAJA,EAIA;AACA,WAAA,sBAAA,GAAA,QAAA;AACA,KANA;AAOA,IAAA,wBAPA,oCAOA,QAPA,EAOA;AACA,WAAA,kBAAA,GAAA,QAAA;AACA,KATA;AAUA,IAAA,cAVA,4BAUA;AACA,UAAA,KAAA,QAAA,KAAA,IAAA,EAAA;AACA,aAAA,QAAA,GAAA,IAAA;AACA,OAFA,MAGA,IAAA,KAAA,QAAA,KAAA,IAAA,EAAA;AACA,aAAA,QAAA,GAAA,IAAA;AACA;AACA,KAjBA;AAkBA,IAAA,kBAlBA,gCAkBA;AACA,WAAA,KAAA,CAAA,QAAA,CAAA,kBAAA;AACA,KApBA;AAqBA,IAAA,eArBA,6BAqBA;AACA,WAAA,KAAA,CAAA,QAAA,CAAA,eAAA;AACA,MAAA,QAAA,CAAA,aAAA,CAAA,MAAA,EAAA,KAAA,CAAA,SAAA,GAAA,QAAA;AACA,KAxBA;AAyBA,IAAA,gBAzBA,8BAyBA;AACA,WAAA,KAAA,CAAA,UAAA,CAAA,KAAA;AACA,WAAA,KAAA,CAAA,cAAA,CAAA,YAAA;AACA,KA5BA;AA6BA,IAAA,cA7BA,0BA6BA,CA7BA,EA6BA;AACA,WAAA,KAAA,CAAA,UAAA,CAAA,IAAA,CAAA,CAAA;AACA,MAAA,QAAA,CAAA,aAAA,CAAA,MAAA,EAAA,KAAA,CAAA,SAAA,GAAA,QAAA;AACA,KAhCA;AAiCA,IAAA,YAjCA,wBAiCA,CAjCA,EAiCA;AAAA;;AACA,MAAA,IAAA,CAAA,EAAA,CAAA,MAAA,EAAA;AACA,QAAA,QAAA,EAAA,WADA;AAEA,QAAA,QAAA,EAAA;AAFA,OAAA;AAIA,MAAA,IAAA,CAAA,EAAA,CAAA,kBAAA,EAAA;AACA,QAAA,OAAA,EAAA,CADA;AAEA,QAAA,QAAA,EAAA;AAFA,OAAA;AAIA,MAAA,IAAA,CAAA,EAAA,CAAA,mBAAA,EAAA;AACA,QAAA,OAAA,EAAA,CADA;AAEA,QAAA,QAAA,EAAA,GAFA;AAGA,QAAA,UAAA,EAAA,sBAAA;AACA,UAAA,KAAA,CAAA,KAAA,CAAA,QAAA,CAAA,gBAAA,GAAA,IAAA;AACA,UAAA,KAAA,CAAA,WAAA,GAAA,CAAA;;AACA,UAAA,KAAA,CAAA,KAAA,CAAA,QAAA,CAAA,YAAA,CAAA,CAAA;AACA;AAPA,OAAA;AASA;AAnDA;AA/CA,CAAA","sourcesContent":["<template>\n  <div id=\"app\">\n    <LoadingCover ref=\"loadingcover\" :percent=\"totalLoadingPercent\" ></LoadingCover>\n    <!-- <QRCodeCover :language=\"this.language\"></QRCodeCover> -->\n    <MyCursor></MyCursor>\n    <Logo></Logo>\n    <Language @switchLanguage=\"switchLanguage\" :language=\"this.language\"></Language>\n    <DetailPage ref=\"detailpage\" @allImagesLoaded = 'detailImagesLoaded' @closeDetailPage = 'closeDetailPage' :language='this.language' :projects = 'this.projects'> </DetailPage>\n    <Header ref=\"header\" @updateLoadingPercent = \"updateLogoLoadingPercent\" :language=\"this.language\"></Header>\n    <Showcase ref=\"showcase\" @loadingCoverHided=\"loadingCoverHided\" @updateLoadingPercent = \"updateShowcaseLoadingPercent\"  @openDetailPage = \"openDetailPage\" @detailPageClosed = \"detailPageClosed\" :language='this.language' :projects = 'this.projects' ></Showcase>\n    <Footer :language=\"this.language\"> </Footer>\n    <ShowcaseFilter ref=\"showcasefilter\" :language=\"this.language\" @changeFilter=\"changeFilter\"></ShowcaseFilter>\n  </div>\n</template>\n\n<script>\n  import MyCursor from \"@/components/MyCursor\";\n  import Header from \"@/components/Header\";\n  import Showcase from \"@/components/Showcase\";\n  import LoadingCover from '@/components/LoadingCover';\n  import Language from '@/components/Language';\n  import Logo from '@/components/Logo';\n  import DetailPage from '@/components/DetailPage';\n  import Footer from '@/components/Footer';\n  import ShowcaseFilter from '@/components/ShowcaseFilter';\n  // import QRCodeCover from '@/components/QRCodeCover';\n\n  import {gsap} from 'gsap'\n  import {ScrollTrigger} from \"gsap/ScrollTrigger\";\n  import {ScrollToPlugin} from \"gsap/ScrollToPlugin\";\n\n  gsap.registerPlugin(ScrollTrigger,ScrollToPlugin);\n\n  export default {\n    name: 'App',\n    components: {\n      Showcase,\n      MyCursor,\n      Header,\n      LoadingCover,\n      Language,\n      Logo,\n      DetailPage,\n      Footer,\n      ShowcaseFilter,\n      // QRCodeCover,\n    },\n    data(){\n      return{\n        showcaseLoadingPercent:0,\n        logoLoadingPercent:0,\n        filterIndex:0,\n        isTitleHide: true,\n        language:'EN',\n      }\n    },\n    computed:{\n      totalLoadingPercent:function(){\n        return (this.showcaseLoadingPercent + this.logoLoadingPercent) / 2\n      },\n      projects:function(){\n        let i = this.filterIndex;\n        let projectTemp = this.ProjectData.data().projectData.reverse().filter(function (item) {\n          return item.isShowcases && item.categoryIndex == i || i == 0;\n        });\n        let arrLength = projectTemp.length;\n        for (let i = 0; i < arrLength - 1; i++) {\n          for (let j = 0; j < arrLength - 1 - i; j++) {\n            if(projectTemp[j].period < projectTemp[j+1].period){\n              let temp = projectTemp[j];\n              projectTemp[j] = projectTemp[j+1];\n              projectTemp[j+1]=temp;\n            }\n          }\n        }\n        return projectTemp;\n      },\n    },\n    mounted(){\n    },\n    methods:{\n      loadingCoverHided(){\n        this.$refs.loadingcover.hide();\n      },\n      updateShowcaseLoadingPercent(newValue){\n        this.showcaseLoadingPercent = newValue\n      },\n      updateLogoLoadingPercent(newValue){\n        this.logoLoadingPercent = newValue\n      },\n      switchLanguage(){\n        if(this.language ==='ZH'){\n          this.language ='EN'\n        }\n        else if(this.language==='EN'){\n          this.language ='ZH'\n        }\n      },\n      detailImagesLoaded(){\n        this.$refs.showcase.detailImagesLoaded();\n      },\n      closeDetailPage(){\n        this.$refs.showcase.closeDetailPage();\n        document.querySelector('#app').style.overflowY = 'scroll';\n      },\n      detailPageClosed(){\n        this.$refs.detailpage.close();\n        this.$refs.showcasefilter.enableFilter();\n      },\n      openDetailPage(e){\n        this.$refs.detailpage.open(e);\n        document.querySelector('#app').style.overflowY = 'hidden';\n      },\n      changeFilter(e){\n        gsap.to('#app',{\n          scrollTo:'.showcase',\n          duration:0.4,\n        });\n        gsap.to('#showcase-canvas',{\n          opacity:0,\n          duration:0.4,\n        });\n        gsap.to('.showcase-wrapper',{\n          opacity:0,\n          duration:0.4,\n          onComplete:()=>{\n            this.$refs.showcase.isChangingFilter = true;\n            this.filterIndex = e;\n            this.$refs.showcase.changeFilter(e);\n          }\n        });\n      }\n    },\n  }\n</script>\n\n<style>\n  #app {\n    -webkit-font-smoothing: antialiased;\n    -moz-osx-font-smoothing: grayscale;\n    text-align: left;\n    width: 100%;\n    height: 100%;\n    box-sizing: border-box;\n    scrollbar-width: none; /* Firefox */\n    -ms-overflow-style: none; /* IE 10+ */\n    overflow-y: scroll; /* has to be scroll, not auto */\n    overflow-x: hidden;\n    -webkit-overflow-scrolling: auto; /* 用来控制元素在移动设备上是否使用滚动回弹效果 */\n\n    z-index: 1;\n  }\n  #app::-webkit-scrollbar{\n    display: none; /* Chrome Safari */\n   }\n\n</style>\n"],"sourceRoot":"src"}]}